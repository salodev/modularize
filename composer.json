{
    "name": "salodev/modularize",
    "description": "Modules feature for laravel",
    "type": "library",
    "require": {
        "laravel/framework": "^8|^9",
        "marcusball/case-helper": "0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Salodev\\Modularize\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Salomón Córdova",
            "email": "info@salomoncordova.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Salodev\\Modularize\\ModulesServiceProvider"
            ]
        }   
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "4.0.x-dev"
    }
}
